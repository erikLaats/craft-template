#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/bin/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/acorn@8.5.0/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/node_modules:/Users/eriklaats/Desktop/Directory/node_modules:/Users/eriklaats/Desktop/node_modules:/Users/eriklaats/node_modules:/Users/node_modules:/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/node_modules/acorn/bin/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/node_modules/acorn/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/bin/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/acorn@8.5.0/node_modules/acorn/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/acorn@8.5.0/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/node_modules:/Users/eriklaats/Desktop/Directory/node_modules:/Users/eriklaats/Desktop/node_modules:/Users/eriklaats/node_modules:/Users/node_modules:/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/node_modules/acorn/bin/node_modules:/Users/eriklaats/Desktop/Directory/craft-template/src/web-application/node_modules/.pnpm/node_modules/acorn/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
